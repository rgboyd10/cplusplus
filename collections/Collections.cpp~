#include <vector>
#include <iostream>

using namespace std;

int main()
{
  vector<int> v;
  v.push_back(3);
  v.push_back(6);
  v.push_back(9);
  v.push_back(2);

  for(auto i = v.begin(); i != v.end(); i++)
    {
      cout << *i << " ";
    }

  cout << endl;

  for(auto i = v.rbegin(); i != v.rend(); i++)
    {
      cout << *i << " ";
    }

  cout << endl;

  int j = v[2];
}
